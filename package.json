{
  "name": "vercel-env-checker",
  "version": "1.0.0",
  "description": "A professional CLI tool to search within environment variable values across Vercel projects",
  "main": "dist/index.js",
  "bin": {
    "vec": "./dist/bin/cli.js",
    "vecw": "./dist/run.js"
  },
  "files": [
